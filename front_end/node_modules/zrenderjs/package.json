{
  "name": "zrenderjs",
  "description": "A lightweight canvas library which providing 2d draw for ECharts。一个轻量级的Canvas类库，MVC封装，数据驱动，提供类Dom事件模型，让canvas绘图大不同！",
  "version": "3.0.3",
  "license": "BSD",
  "main": "zrender",
  "scripts": {
    "build": "npm run lib && npm run dist",
    "dist": "gulp dist-clean && npm run dist-build",
    "dist-build": "node node_modules/requirejs/bin/r -o build/build.js && uglifyjs dist/zrender.js -m -c -o dist/zrender.min.js",
    "lib": "gulp lib",
    "prerelease": "npm run dist && gulp prerelease",
    "release": "npm run prerelease && cd .publish && npm publish",
    "test": "jshint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/luqin/zrender"
  },
  "devDependencies": {
    "del": "^2.0.0",
    "gulp": "^3.9.0",
    "gulp-plumber": "^1.0.1",
    "gulp-replace": "^0.5.4",
    "jshint": "^2.8.0",
    "requirejs": "^2.1.22",
    "run-sequence": "^1.1.5",
    "uglify-js": "^2.6.1"
  },
  "keywords": [
    "echarts",
    "chart",
    "graph",
    "webpack",
    "canvas",
    "zrender",
    "svg",
    "vml",
    "2d"
  ],
  "author": [{
    "name": "Kenner",
    "email": "kener.linfeng@gmail.com"
  }, {
    "name": "Yi Shen",
    "url": "https://github.com/pissang"
  }, {
    "name": "Shuang Su",
    "url": "https://github.com/100pah"
  }],
  "contributors": [{
    "name": "erik",
    "email": "errorrik@gmail.com"
  }]
}
