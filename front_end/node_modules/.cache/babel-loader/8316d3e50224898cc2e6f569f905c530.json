nted"],"sources":["src/components/blog_detail/sentiment.vue"],"sourcesContent":["<template>\r\n  <div class=\"hot_point\">\r\n    <div class=\"hot_point_title\">情感分析</div>\r\n    <div class=\"hot_point_contents\">\r\n      <div class=\"sentiment_chart\" ref=\"sentimentChart\"></div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport * as echarts from 'echarts';\r\n\r\nexport default {\r\n  name: \"hot_point\",\r\n  data() {\r\n    return {\r\n      sentimentData: {\r\n        positive: 0,\r\n        negative: 0,\r\n        neutral: 0\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    // 获取当前博文的情感分析数据\r\n    getComments() {\r\n      let query = this.$route.query;\r\n      this.$axios\r\n        .get(\"comment/sentiment/statistics\", {\r\n          params: {\r\n            tag_task_id: query.tag_task_id,\r\n            weibo_id: query.weibo_id,\r\n          }\r\n        })\r\n        .then((res) => {\r\n          console.log(res);\r\n          // 获取情感分析数据（百分比）\r\n          const sentimentData = res.data.data; // 直接获取情感统计结果\r\n          this.sentimentData.positive = sentimentData.positive * 100;  // 转换为百分比\r\n          this.sentimentData.negative = sentimentData.negative * 100;\r\n          this.sentimentData.neutral = sentimentData.neutral * 100;\r\n\r\n          // 绘制饼状图\r\n          this.drawSentimentChart();\r\n        })\r\n        .catch(error => {\r\n          console.error(\"获取评论情感分析失败:\", error);\r\n        });\r\n    },\r\n\r\n    // 绘制饼状图\r\n    drawSentimentChart() {\r\n      const chartElement = this.$refs.sentimentChart;\r\n      const myChart = echarts.init(chartElement);\r\n      const option = {\r\n        title: {\r\n          text: '情感分析',\r\n          left: 'center'\r\n        },\r\n        tooltip: {\r\n          trigger: 'item',\r\n          formatter: '{a} <br/>{b}: {c} ({d}%)'\r\n        },\r\n        series: [\r\n          {\r\n            name: '情感分析',\r\n            type: 'pie',\r\n            radius: ['40%', '70%'],\r\n            label: {\r\n              position: 'outside',\r\n              formatter: '{b}: {d}%'\r\n            },\r\n            data: [\r\n              { value: this.sentimentData.positive, name: 'Positive' },\r\n              { value: this.sentimentData.negative, name: 'Negative' },\r\n              { value: this.sentimentData.neutral, name: 'Neutral' }\r\n            ]\r\n          }\r\n        ]\r\n      };\r\n      myChart.setOption(option);\r\n    },\r\n  },\r\n  mounted() {\r\n    this.getComments();  // 在组件加载时获取评论情感分析数据\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.hot_point {\r\n  position: absolute;\r\n  width: 100%;\r\n  height: 54%;\r\n  top: 46%;\r\n  background-color: #fff;\r\n}\r\n\r\n.hot_point_title {\r\n  margin: 10px 20px;\r\n  padding: 5px;\r\n  font-weight: 600;\r\n  letter-spacing: 1px;\r\n}\r\n\r\n.hot_point_contents {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  height: 100%;\r\n}\r\n\r\n.sentiment_chart {\r\n  width: 80%;\r\n  height: 80%;\r\n}\r\n</style>\r\n"],"mappings":"AAUA,YAAAA,OAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,aAAA;QACAC,QAAA;QACAC,QAAA;QACAC,OAAA;MACA;IACA;EACA;EACAC,OAAA;IACA;IACAC,YAAA;MACA,IAAAC,KAAA,QAAAC,MAAA,CAAAD,KAAA;MACA,KAAAE,MAAA,CACAC,GAAA;QACAC,MAAA;UACAC,WAAA,EAAAL,KAAA,CAAAK,WAAA;UACAC,QAAA,EAAAN,KAAA,CAAAM;QACA;MACA,GACAC,IAAA,CAAAC,GAAA;QACAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;QACA;QACA,MAAAd,aAAA,GAAAc,GAAA,CAAAf,IAAA,CAAAA,IAAA;QACA,KAAAC,aAAA,CAAAC,QAAA,GAAAD,aAAA,CAAAC,QAAA;QACA,KAAAD,aAAA,CAAAE,QAAA,GAAAF,aAAA,CAAAE,QAAA;QACA,KAAAF,aAAA,CAAAG,OAAA,GAAAH,aAAA,CAAAG,OAAA;;QAEA;QACA,KAAAc,kBAAA;MACA,GACAC,KAAA,CAAAC,KAAA;QACAJ,OAAA,CAAAI,KAAA,gBAAAA,KAAA;MACA;IACA;IAEA;IACAF,mBAAA;MACA,MAAAG,YAAA,QAAAC,KAAA,CAAAC,cAAA;MACA,MAAAC,OAAA,GAAA1B,OAAA,CAAA2B,IAAA,CAAAJ,YAAA;MACA,MAAAK,MAAA;QACAC,KAAA;UACAC,IAAA;UACAC,IAAA;QACA;QACAC,OAAA;UACAC,OAAA;UACAC,SAAA;QACA;QACAC,MAAA,GACA;UACAlC,IAAA;UACAmC,IAAA;UACAC,MAAA;UACAC,KAAA;YACAC,QAAA;YACAL,SAAA;UACA;UACAhC,IAAA,GACA;YAAAsC,K